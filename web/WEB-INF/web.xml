<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EbNewsController</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbNewsController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbNewsController</servlet-name>
        <url-pattern>/news.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EbCProductServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbCProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbCProductServlet</servlet-name>
        <url-pattern>/pca.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EbNoteServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbNoteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbNoteServlet</servlet-name>
        <url-pattern>/note.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EbProductServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbProductServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbProductServlet</servlet-name>
        <url-pattern>/product.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EbCommentServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbCommentServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbCommentServlet</servlet-name>
        <url-pattern>/comment.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbCartServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbCartServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbCartServlet</servlet-name>
        <url-pattern>/cart.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EbInsertUserServlet</servlet-name>
        <servlet-class>sdkd.com.ec.controller.EbInsertUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EbInsertUserServlet</servlet-name>
        <url-pattern>/insertu.do</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>news.do</welcome-file>
    </welcome-file-list>
</web-app>